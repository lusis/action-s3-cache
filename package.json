{
  "name": "action-s3-cache",
  "version": "2.0.0",
  "private": true,
  "description": "Cache dependencies and build outputs to S3",
  "scripts": {
    "build": "tsc",
    "release": "ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "test": "echo \"TODO: Add tests\"",
    "lint": "eslint **/*.ts --cache",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saniales/action-s3-cache"
  },
  "keywords": [
    "actions",
    "node",
    "cache"
  ],
  "author": "saniales",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.2.2",
    "@actions/core": "^1.10.1",
    "@actions/io": "^1.1.3",
    "@aws-sdk/client-s3": "^3.523.0",
    "graceful-fs": "^4.2.11",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.10.8",
    "@types/tar": "^6.1.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "nock": "^13.4.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
